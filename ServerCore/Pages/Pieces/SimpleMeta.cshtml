@page "/{eventId}/{eventRole}/Teams/{teamId}/SimpleMeta/{puzzleId}"
@model ServerCore.Pages.Pieces.SimpleMetaModel

@{
    ViewData["Title"] = $"{Model.Puzzle.PlaintextName} Meta";
}

<h2>@ServerCore.Helpers.RawHtmlHelper.Display(@Model.Puzzle.Name, Model.Event.ID, Html) Pieces Earned</h2>

<table class="table">
    <tbody>
        @foreach (var item in Model.EarnedPieces) {
            <tr>
                <td class="meta-list-piece-customizable">
                    @Html.Raw(item.Contents)
                </td>
            </tr>
        }
    </tbody>
</table>
