@page "/{eventId}/{eventRole}/Events/Import"
@model ServerCore.Pages.Events.ImportModel

@{
    ViewData["Title"] = "Import event";
    ViewData["AdminRoute"] = "/Events/Import";
    ViewData["AuthorRoute"] = "/EventSpecific/Index";
    ViewData["PlayRoute"] = "/EventSpecific/Index";
}

<h2>@Model.Event.Name: Import</h2>

<form method="post" enctype="multipart/form-data">
    <input type="hidden" asp-for="Event.ID" />
    <select class="form-control" asp-for="ImportEventID" asp-items="@(new SelectList(Model.Events, "ID", "Name"))"></select>
    <input type="submit" value="Import" asp-page-handler="Import" class="btn btn-default" />
</form>